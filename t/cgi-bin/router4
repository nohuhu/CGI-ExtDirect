#!/bin/sh

$PERL -x 3<&0 <<'END_OF_SCRIPT'
#!perl

use CGI::ExtDirect;

use RPC::ExtDirect::Test::Pkg::Meta;

# 2 argument open() is here for older Perls
open STDIN, '<&3' or die "Can't reopen STDIN";

my %headers = ();

my $cgi = CGI::ExtDirect->new({ debug => 1 });

print $cgi->route(%headers);

exit 0;

END_OF_SCRIPT

